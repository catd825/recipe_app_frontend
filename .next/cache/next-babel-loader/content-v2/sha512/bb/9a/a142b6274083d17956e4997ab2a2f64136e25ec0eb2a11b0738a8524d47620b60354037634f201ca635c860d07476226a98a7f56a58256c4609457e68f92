ipes.map(recipe => {\n                if(mapFavoriteRecipeIds().includes(recipe.id)){\n                    return <RecipeItem key={recipe.id} recipes={recipe} />\n                } else {\n                    return null\n                }\n            })\n        }\n    }\n\n\n    return (\n        <div style={{textAlign: 'center'}}>\n            <div className=\"cards\">\n                {recipeCard()}\n            </div>\n        </div>\n    )\n}\n"]},"metadata":{},"sourceType":"module"}